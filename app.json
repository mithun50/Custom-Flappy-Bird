{
  "expo": {
    "name": "FlappyBird",
    "slug": "FlappyBird",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_bird.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_bird.png",
      "resizeMode": "contain",
      "backgroundColor": "#70c5ce"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.master.supercat",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_bird.png",
        "backgroundColor": "#70c5ce"
      }
    },
    "web": {
      "favicon": "./assets/logo_bird.png"
    }
  }
}
